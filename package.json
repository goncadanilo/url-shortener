    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "faker": "^4.1.0",
    "husky": "^4.2.5",
    "jest": "^25.5.3",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
